    "express-rate-limit": "^8.1.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "dotenv": "^17.2.2",
    "mongodb": "^6.20.0",
    "nodemailer": "^7.0.6",
    "resend": "^6.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
